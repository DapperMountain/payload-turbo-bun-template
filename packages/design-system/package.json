{
  "name": "@dappermountain/design-system",
  "version": "1.0.0",
  "description": "Dapper Mountain Design System",
  "private": true,
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "types",
    "src",
