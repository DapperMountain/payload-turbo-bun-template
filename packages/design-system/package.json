    "@tamagui/next-theme": "^1.132.17",
    "@tamagui/shorthands": "^1.132.17",
    "@tamagui/theme-builder": "^1.132.17",
    "lucide-react": "^0.532.0",
    "tamagui": "^1.132.17"
  },
  "devDependencies": {
    "@dappermountain/typescript-config": "*",
    "@tamagui/build": "^1.132.17"
  },
  "peerDependencies": {
    "react": ">= 19.0.0",
    "react-dom": ">= 19.0.0",
    "react-native": "^0.80.2"
